{
  "private": true,
  "name": "vci-logcat",
  "description": "VCI logcat",
  "version": "0.10.1",
  "author": "oO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oocytanb/vci-logcat.git"
  },
  "bin": "bin/vci-logcat",
  "main": "bin/lib/index.js",
  "types": "bin/lib/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "pkg": {
    "scripts": "bin/**/*.js",
    "outputPath": "dist"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "lint": "eslint . && prettier -c src spec",
    "lint:fix": "prettier --write src spec && eslint . --fix",
    "test:package_info": "mocha --exclude './**' --file ./spec/package_info.spec.ts",
    "test": "mocha",
    "start": "ts-node ./src/node/cli_main.ts",
    "retail": "node ./bin/vci-logcat",
    "coverage": "nyc npm run test",
    "coverage:clean": "rimraf .nyc_output coverage",
    "clean": "rimraf ./bin/*/ && rimraf ./dist && npm run coverage:clean",
    "build:package_info": "ts-node ./build_scripts/generate_package_info.ts",
    "build": "npm run build:package_info && npm run lint:fix && npm run clean && tsc",
    "pack:d": "npm pack --dry-run"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.8",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@cycle/run": "^5.5.0",
    "@types/msgpack-lite": "^0.1.8",
    "@types/ramda": "^0.28.7",
    "@types/ws": "^8.2.0",
    "chalk": "^4.1.2",
    "commander": "^9.1.0",
    "isomorphic-ws": "^4.0.1",
    "mocha": "^9.0.3",
    "moment": "^2.29.1",
    "msgpack-lite": "^0.1.26",
    "ramda": "^0.28.0",
    "ws": "^8.0.0",
    "xstream": "^11.14.0"
  },
  "keywords": [
    "vci"
  ],
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "script"
      }
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
